{
  "name": "@fractaldb/fractal-server",
  "version": "0.2.7",
  "main": "dist/index",
  "types": "dist/index",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist && rimraf ./tsconfig.tsbuildinfo",
    "watch": "tsc -w",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "test": "node --experimental-specifier-resolution=node --loader ts-node/esm ./tests.test.ts"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "./tests/tsconfig.json",
        "useESM": true
      }
    }
  },
  "dependencies": {
    "@fractaldb/adn": "^0.2.7",
    "@fractaldb/shared": "^0.2.7",
    "@framework-tools/catchit": "^1.0.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "ts-node": "^10.0.0"
  },
  "gitHead": "7d986ba599dc0014b81e473d076b504dbca27cd6",
  "devDependencies": {
    "ts-jest": "^27.0.3"
  }
}
