{
  "name": "@fractaldb/shared",
  "version": "0.3.1",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "./dist/index.d.ts": [
        "./dist/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist && rimraf ./tsconfig.tsbuildinfo",
    "watch": "tsc -w",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "test": "npm run build"
  },
  "dependencies": {
    "@fractaldb/adn": "^0.3.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4"
  },
  "gitHead": "0607a4ea8ef92688d1c8af3daf7f2300b41edbbe"
}
