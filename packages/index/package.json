{
  "name": "@fractaldb/indexing-system",
  "version": "0.2.13",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "./dist/index.d.ts": [
        "./dist/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist && rimraf ./tsconfig.tsbuildinfo",
    "watch": "tsc -w",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "test": "npm run build"
  },
  "dependencies": {
    "@fractaldb/fractal-server": "^0.2.13",
    "@framework-tools/catchit": "^1.0.11",
    "ts-node": "^10.0.0"
  }
}