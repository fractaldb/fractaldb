{
  "name": "@fractaldb/fractal-server",
  "version": "0.3.1",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "./dist/index.d.ts": [
        "./dist/index.d.ts"
      ],
      "*": [
        "dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist && rimraf ./tsconfig.tsbuildinfo",
    "watch": "tsc -w",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "test": "node --loader ts-node/esm ./tests/tests.test.ts"
  },
  "dependencies": {
    "@fractaldb/adn": "^0.3.1",
    "@fractaldb/indexing-system": "^0.3.1",
    "@fractaldb/shared": "^0.3.1",
    "@framework-tools/catchit": "^1.0.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "crc-32": "^1.2.0",
    "ts-node": "^10.0.0"
  },
  "gitHead": "0607a4ea8ef92688d1c8af3daf7f2300b41edbbe",
  "devDependencies": {
    "ts-jest": "^27.0.3"
  }
}
